{
    "name": "tridah/budget-tracker",
    "description": "A free, open-source budget tracking application for personal and business finance management",
    "type": "project",
    "keywords": [
        "budget",
        "finance",
        "tracking",
        "expense",
        "income",
        "budgeting",
        "money management"
    ],
    "homepage": "https://github.com/TridahCloud/BudgetTracker",
    "license": "MIT",
    "authors": [
        {
            "name": "Tridah",
            "email": "info@tridah.cloud",
            "homepage": "https://tridah.cloud",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/TridahCloud/BudgetTracker/issues",
        "source": "https://github.com/TridahCloud/BudgetTracker",
        "docs": "https://github.com/TridahCloud/BudgetTracker/blob/main/README.md"
    },
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Tridah\\BudgetTracker\\": "api/classes/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-security": "composer audit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

